{
    "bootstrapVersion": 3,
    "logLevel": "debug",
    "appStyles": "./src/scss/main.scss",
    "env": {
        "development": {
            "extractStyles": false,
            "styleLoaders": [ 
                "style-loader",
                "css-loader?sourceMap",
                "sass-loader?sourceMap"
            ]
        },
        "production": {
            "extractStyles": true,
            "styleLoaders": [ 
                "style-loader",
                "css-loader",
                "sass-loader"
            ]
        }
    },
    "styles": {
        "normalize": true,
        "print": true,
        "mixins": true,
        "grid": true
    },
    "scripts": {
        "alert": true,
        "button": true
    }
}